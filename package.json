{
  "name": "carrier",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "configureJPush": "node node_modules/jpush-react-native/JPushConfiguration.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "cd android && rm -rf app/src/main/res/drawable-* && ./gradlew assembleRelease",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:staging": "npm run android:dist && cd android && ./gradlew assembleStaging",
    "android:dist": "react-native bundle --entry-file index.android.js --bundle-output ./android/app/src/main/assets/index.android.jsbundle --platform android --assets-dest ./android/app/src/main/res/ --dev false",
    "watchman": "watchman watch-del /Users/kaisun/Desktop/llmj-carrier ; watchman watch-project /Users/kaisun/Desktop/llmj-carrier",
    "ios-debug": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/shipper/main.jsbundle' --dev=true --platform='ios' --assets-dest='./ios'",
    "preios-dist": "node script/prerelease_ios",
    "ios-dist": "rm -rf dist && mkdir dist && react-native bundle --entry-file ./index.ios.js --bundle-output ./dist/main.jsbundle --platform ios --assets-dest ./dist --dev false",
    "deployment:ios": "code-push deployment ls carrier-ios -k",
    "deployment:android": "code-push deployment ls carrier-android -k",
    "collaboration": "code-push collaborator add <appName> <collaboratorEmail>",
    "predeploy:ios": "node script/prebuild_ios",
    "deploy:ios": "code-push release-react carrier-ios ios --dev false --deploymentName Staging --mandatory false --description ",
    "deploy:ios:dev": "code-push release-react carrier-ios ios --description '修复一些问题' --dev true --mandatory false",
    "predeploy:android": "node script/prebuild_and",
    "deploy:android": "code-push release-react carrier-android android --description '紧急修复bug,请及时更新' --dev false --deploymentName Production --mandatory true",
    "deploy:android:dev": "code-push release-react carrier-android android --dev true --mandatory false --description",
    "clean:ios": "code-push deployment clear carrier-ios Staging",
    "clean:android": "code-push deployment clear carrier-android Staging"
  },
  "dependencies": {
    "@remobile/react-native-toast": "^1.0.7",
    "apsl-react-native-button": "^3.1.0",
    "axios": "^0.18.0",
    "coordtransform": "^2.1.2",
    "immutable": "^3.8.2",
    "jcore-react-native": "^1.2.5",
    "jpush-react-native": "^2.1.11",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.2",
    "react-native-baidu-map-xzx": "^2.2.0",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-code-push": "^5.0.0-beta",
    "react-native-communications": "^2.2.1",
    "react-native-device-info": "^0.11.0",
    "react-native-drawer": "^2.5.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.5.1",
    "react-native-image-crop-picker": "^0.16.0",
    "react-native-image-picker": "^0.26.7",
    "react-native-image-zoom-viewer": "^2.0.20",
    "react-native-img-cache": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-pdf": "^1.2.1",
    "react-native-photo-view": "^1.5.2",
    "react-native-picker": "4.0.19",
    "react-native-picker-custom": "^1.1.0",
    "react-native-root-modal": "1.1.1",
    "react-native-root-siblings": "1.2.1",
    "react-native-root-toast": "1.3.0",
    "react-native-scrollable-tab-view": "^0.6.7",
    "react-native-snap-carousel": "^3.6.0",
    "react-native-splash-screen": "^3.0.0",
    "react-native-swipeout": "^2.3.3",
    "react-native-swiper": "^1.5.10",
    "react-native-switch": "^1.1.0",
    "react-native-tab-navigator": "^0.3.4",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "3.0.2",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
